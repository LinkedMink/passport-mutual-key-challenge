{
  "name": "@linkedmink/passport-mutual-key-challenge",
  "version": "0.1.0",
  "description": "Implements a Passport strategy to authenticate the public key of a user by issuing a dynamic generated challenge",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "Harlan Sang",
    "email": "harlan@linkedmink.space"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LinkedMink/passport-mutual-key-challenge"
  },
  "scripts": {
    "start": "cd demo && yarn run start",
    "startClient": "cd demo && yarn run startClient",
    "build": "tsc --project ./tsconfig.json",
    "test": "jest",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:demo": "cd demo && yarn run lint",
    "format": "prettier --write src/**/*.ts tests/**/*.ts *.js *.json",
    "format:demo": "cd demo && yarn run format",
    "link": "yarn run build && yarn link && yarn link @linkedmink/passport-mutual-key-challenge",
    "prepublishOnly": "yarn run build && node incrementVersion.js",
    "doc": "typedoc"
  },
  "dependencies": {
    "passport-strategy": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.27",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-strategy": "^0.2.35",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "ts-jest": "^26.5.0",
    "ts-node-dev": "^1.1.1",
    "typedoc": "^0.20.23",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  }
}
